{
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended",
	],
	"parser": "babel-eslint",
	"plugins": [
		"prettier",
		"eslint-plugin-import-order-alphabetical",
	],
	"rules": {
		"import-order-alphabetical/order": ["error"],
	}
}
